int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int a,b,sum,n,test;
    cin>>test;
    for(int i=1;i<=test;i++)
    {   
        cin>>a>>b>>n;
        sum=0;
        for(int j=1;j<=n;j++)
        {
            sum+=pow(2,j-1)*b;
            cout<<a+sum<<" ";
        }
    cout<<"\n";
                
}   
    return 0;
}
